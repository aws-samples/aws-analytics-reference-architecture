{"Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowEMRStudioAccesstoS3BucketforStudioWorkspaces",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:GetEncryptionConfiguration",
        "s3:ListBucket",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::<your-amazon-s3-bucket>",
        "arn:aws:s3:::<your-amazon-s3-bucket>/*"
      ]
    }
  ]
}
